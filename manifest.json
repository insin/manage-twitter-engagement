{
  "manifest_version": 2,
  "name": "Manage Twitter Engagement",
  "description": "Manage extra \"engagement\" on Twitter by hiding Retweets and algorithmic timeline tweets (Retweetlikes)",
  "homepage_url": "https://github.com/insin/manage-twitter-engagement/",
  "version": "1.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/"
      ],
      "js": ["./manage-twitter-engagement.user.js"]
    }
  ]
}
